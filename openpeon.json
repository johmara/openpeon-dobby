  "language": "en",
  "description": "Dobby is here to help! A house-elf themed sound pack for OpenCode featuring Dobby from Harry Potter.",
  "categories": {
    "session.start": {
      "sounds": [
        {
          "file": "sounds/dobby-ready.wav",
          "label": "Dobby is here to help, Master!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-hello.wav",
          "label": "Dobby is free to serve!",
          "sha256": ""
        }
      ]
    },
    "task.acknowledge": {
      "sounds": [
        {
          "file": "sounds/dobby-yes.wav",
          "label": "Yes, Master!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-will-do.wav",
          "label": "Dobby will do it!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-right-away.wav",
          "label": "Right away, sir!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-happy-to-help.wav",
          "label": "Dobby is happy to help!",
          "sha256": ""
        }
      ]
    },
    "task.complete": {
      "sounds": [
        {
          "file": "sounds/dobby-finished.wav",
          "label": "Dobby has finished!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-done.wav",
          "label": "Task completed, sir!",
          "sha256": ""
