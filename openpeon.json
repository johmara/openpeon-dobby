  "description": "Dobby is here to help! A house-elf themed sound pack for OpenCode featuring Dobby from Harry Potter.",
  "categories": {
    "session.start": {
      "sounds": [
        {
          "file": "sounds/dobby-ready.wav",
          "label": "Dobby is here to help, Master!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-hello.wav",
          "label": "Dobby is free to serve!",
          "sha256": ""
        }
      ]
    },
    "task.complete": {
      "sounds": [
        {
          "file": "sounds/dobby-done.wav",
          "label": "Task completed, sir!",
          "sha256": ""
        }
      ]
    },
    "task.error": {
      "sounds": [
        {
          "file": "sounds/dobby-difficult.wav",
          "label": "This is difficult for Dobby!",
          "sha256": ""
        }
      ]
    },
    "input.required": {
      "sounds": [
        {
          "file": "sounds/dobby-where-to-begin.wav",
          "label": "Where to begin?",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-understands.wav",
          "label": "Dobby understands!",
          "sha256": ""
        }
      ]
    }
  }
