    "task.error": {
      "sounds": [
        {
          "file": "sounds/dobby-failed.wav",
          "label": "Dobby has failed you!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-bad.wav",
          "label": "Bad Dobby!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-sorry.wav",
          "label": "Dobby is so sorry!",
          "sha256": ""
        }
      ]
    },
    "input.required": {
      "sounds": [
        {
          "file": "sounds/dobby-what-can-do.wav",
          "label": "What can Dobby do for you?",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-listening.wav",
          "label": "Dobby is listening...",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-need-help.wav",
          "label": "Does Master need Dobby's help?",
          "sha256": ""
        }
      ]
    },
    "resource.limit": {
      "sounds": [
        {
          "file": "sounds/dobby-cannot.wav",
          "label": "Dobby cannot do this!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-too-much.wav",
          "label": "Too much for Dobby!",
          "sha256": ""
        }
      ]
    },
    "user.spam": {
      "sounds": [
        {
          "file": "sounds/dobby-overwhelmed.wav",
          "label": "Dobby is overwhelmed!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-too-many.wav",
          "label": "Too many tasks at once!",
          "sha256": ""
        },
        {
          "file": "sounds/dobby-slow-down.wav",
          "label": "Please, Master, slow down!",
          "sha256": ""
        }
      ]
    }
